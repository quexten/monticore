grammar InterfaceSig extends de.monticore.MCBasics {

// ----------------------- Interface signature

  interface I =     x:Integer        y:Name* ;
  A implements I =  x:Integer "...1" y:(Name || ",")* ;
  B implements I =  y:Name*   "...2" x:Integer        "..." ;
  C = I "..." ;

  Integer = "..." ;
}


