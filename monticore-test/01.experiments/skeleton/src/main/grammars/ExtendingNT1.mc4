grammar ExtendingNT1 extends de.monticore.MCBasics {

// ----------------------- Extension of NTs

  A = "...1";
  B extends A = "...2";
  C = A;

// ----------------------- Extension of NTs

  abstract AutomatonElement;

  State extends AutomatonElement = "...1" ;

  Transition extends AutomatonElement = "...2" ;

}

